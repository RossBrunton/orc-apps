<?xml version="1.0" encoding="UTF-8"?>
<XDF name="connected_components">
    <Instance id="FileReader">
        <Class name="std.stdio.FileReader"/>
    </Instance>
    <Instance id="EndOfStream">
        <Class name="std.stdio.EndOfStream"/>
    </Instance>
    <Instance id="FileWriter">
        <Class name="std.stdio.Writer"/>
    </Instance>
    <Instance id="Above">
        <Class name="image.analysis.connected_components.Above"/>
    </Instance>
    <Instance id="Threshold">
        <Class name="image.point.threshold.binaryThreshold"/>
    </Instance>
    <Instance id="Label">
        <Class name="image.analysis.connected_components.Label"/>
    </Instance>
    <Connection dst="FileWriter" dst-port="Byte" src="EndOfStream" src-port="Out"/>
    <Connection dst="FileWriter" dst-port="pEOF" src="EndOfStream" src-port="pEOF"/>
    <Connection dst="Threshold" dst-port="Gin" src="FileReader" src-port="O"/>
    <Connection dst="Label" dst-port="Gin" src="Threshold" src-port="Gout"/>
    <Connection dst="EndOfStream" dst-port="In" src="Label" src-port="Gout"/>
    <Connection dst="Above" dst-port="Gin" src="Label" src-port="Gout"/>
    <Connection dst="Label" dst-port="Above" src="Above" src-port="Gout"/>
</XDF>
